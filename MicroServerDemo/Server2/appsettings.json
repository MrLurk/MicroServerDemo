{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConsulServerConfig": {
    "ServiceName": "Service2",
    "ServiceIP": "localhost",
    "ServicePort": 5005,
    "ServiceHealthCheck": "https://localhost:5005/consulcheck",
